{
  "version": 2,
  "builds": [
    {
      "src": "src/YourApp.csproj",
      "use": "@vercel/dotnet",
      "config": {
        "dotnetVersion": "6.0"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
